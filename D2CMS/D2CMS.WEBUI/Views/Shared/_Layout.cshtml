<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="">
    <!-- The above 6 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- Title -->
    <title>@ViewData["Title"] | D2CMS</title>

    <!-- Styles -->
    <link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">
    <link href="@Url.Content("~/assets/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/assets/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/assets/plugins/icomoon/style.css")" rel="stylesheet">
    <link href="@Url.Content("~/assets/plugins/uniform/css/default.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/plugins/switchery/switchery.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/plugins/nvd3/nv.d3.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/assets/plugins/toastr/toastr.min.css")" rel="stylesheet">
    <!-- Theme Styles -->
    <link href="@Url.Content("~/assets/css/space.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/assets/css/custom.css")" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("Styles", false)
</head>
<body>

    <!-- Page Container -->
    <div class="page-container">
        @Html.Partial("_Sidebar")
        <!-- Page Content -->
        <div class="page-content">
            <!-- Page Header -->
            <div class="page-header">
                @*<div class="search-form">
                    <form action="#" method="GET">
                        <div class="input-group">
                            <input type="text" name="search" class="form-control search-input" placeholder="Type something...">
                            <span class="input-group-btn">
                                <button class="btn btn-default" id="close-search" type="button"><i class="icon-close"></i></button>
                            </span>
                        </div>
                    </form>
                </div>*@
                <nav class="navbar navbar-default">
                    <div class="container-fluid">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <div class="logo-sm">
                                <a href="javascript:void(0)" id="sidebar-toggle-button"><i class="fa fa-bars"></i></a>
                                <a class="logo-box" href="@Url.Action("Index","Home")"><span>D2CMS</span></a>
                            </div>
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                <i class="fa fa-angle-down"></i>
                            </button>
                        </div>

                        <!-- Collect the nav links, forms, and other content for toggling -->

                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                <li><a href="javascript:void(0)" id="collapsed-sidebar-toggle-button"><i class="fa fa-bars"></i></a></li>
                                <li><a href="javascript:void(0)" id="toggle-fullscreen"><i class="fa fa-expand"></i></a></li>
                                @*<li><a href="javascript:void(0)" id="search-button"><i class="fa fa-search"></i></a></li>*@
                            </ul>
                            <ul class="nav navbar-nav navbar-right">
                                <li class="dropdown user-dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Xin chào @User.Identity.Name 様 </a>
                                    <ul class="dropdown-menu">
                                        <li><a href="@Url.Action("MyProfile","User")">Profile</a></li>                                        
                                        <li role="separator" class="divider"></li>
                                        <li><a href="@Url.Action("Settings","User")">Account Settings</a></li>
                                        <li><a href="@Url.Action("Logout","Authen")">Log Out</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </div><!-- /.container-fluid -->
                </nav>
            </div>
            <!-- /Page Header -->
            <!-- Page Inner -->
            <div class="page-inner">
                @RenderBody()
                <div class="page-footer">
                    <p>Made with <i class="fa fa-heart"></i> by stacks</p>
                </div>
            </div><!-- /Page Inner -->

        </div><!-- /Page Content -->
    </div><!-- /Page Container -->
    <!-- Javascripts -->
    <script src="@Url.Content("~/assets/plugins/jquery/jquery-3.1.0.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/bootstrap/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/jquery-slimscroll/jquery.slimscroll.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/uniform/js/jquery.uniform.standalone.js")"></script>
    <script src="@Url.Content("~/assets/plugins/switchery/switchery.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/d3/d3.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/nvd3/nv.d3.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/flot/jquery.flot.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/flot/jquery.flot.time.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/flot/jquery.flot.symbol.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/flot/jquery.flot.resize.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/flot/jquery.flot.tooltip.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/flot/jquery.flot.pie.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/chartjs/chart.min.js")"></script>
    <script src="@Url.Content("~/assets/js/space.min.js")"></script>
    <script src="@Url.Content("~/assets/js/pages/dashboard.js")"></script>
    <script src="@Url.Content("~/assets/plugins/toastr/toastr.min.js")"></script>
    @RenderSection("Scripts", false)
</body>
</html>